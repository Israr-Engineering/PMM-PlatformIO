      <pou name="SetInternalRTC" pouType="functionBlock">
        <interface>
          <inputVars>
            <variable name="YEAR"><type><INT/></type></variable>
            <variable name="MONTH"><type><INT/></type></variable>
            <variable name="DAY"><type><INT/></type></variable>
            <variable name="HOUR"><type><INT/></type></variable>
            <variable name="MINTS"><type><INT/></type></variable>
            <variable name="SECONDs"><type><INT/></type></variable>
          </inputVars>
          <outputVars>
            <variable name="DATE"><type><DATE/></type></variable>
            <variable name="TIME"><type><TIME/></type></variable>
            <variable name="SUCCESS"><type><SINT/></type></variable>
          </outputVars>
        </interface>
        <body>
          <ST>
            <xhtml:p><![CDATA[SUCCESS := 0;]]></xhtml:p>
          </ST>
        </body>
        <documentation>
          <xhtml:p><![CDATA[Set internal RTC for Samd21 devices. If SUCCESS is zero, an error has occurred, or there aren't any modules on the bus]]></xhtml:p>
        </documentation>
      </pou>

      
